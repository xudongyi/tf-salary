<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="business.mapper.HrmResourceMapper">
    <select id="getHrmResource" resultType="Map">
        select * from hrmresource where loginid=#{loginId}
    </select>
    <select id="getHrmResourceByWorkcode" resultType="Map">
        select * from hrmresource where workcode = #{workcode}
    </select>
</mapper>